{
    "Name": "FunOS",
    "URL": "fungible://FunOS",
    "BuildTool": "make",
    "BuildVerbosityFormat": "2Vl=1",
    "BuildQuietFormat": "1dq",
    "BuildCommand": "make -j%NCPU% MACHINE=%TARGET% SIGN=1",
    "CleanCommand": "make clean",
    "BuildDirectory": "./build",
    "TagPrefix": "bld_",
    "Images":
    {
        "default": "stripped",
        "rich": "funos-%TARGET%",
        "stripped": "funos-%TARGET%.stripped",
        "signed": "funos-%TARGET%.signed"
    }
}